{
    "name": "amphp/http-server-form-parser",
    "homepage": "https://github.com/amphp/http-server-form-parser",
    "description": "A form parser for Amp's HTTP parser.",
    "keywords": [
        "http",
        "form",
        "async",
        "non-blocking",
        "amp",
        "amphp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Lowrey",
            "email": "rdlowrey@php.net"
        },
        {
            "name": "Bob Weinand"
        },
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        },
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "amphp/amp": "^3-dev",
        "amphp/byte-stream": "^2-dev",
        "amphp/http": "^1",
        "amphp/http-server": "^3-dev"
    },
    "require-dev": {
        "amphp/php-cs-fixer-config": "dev-master",
        "amphp/phpunit-util": "^3-dev",
        "phpunit/phpunit": "^9",
        "amphp/log": "^2-dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Amp\\Http\\Server\\FormParser\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Http\\Server\\FormParser\\Test\\": "test"
        }
    },
	"scripts": {
        "test": "@php -dzend.assertions=1 -dassert.exception=1 ./vendor/bin/phpunit",
        "code-style": "@php ./vendor/bin/php-cs-fixer fix"
    }
}
