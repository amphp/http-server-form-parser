{
    "name": "amphp/http-server-form-parser",
    "homepage": "https://amphp.org/http-server-form-parser",
    "description": "An HTTP server plugin that simplifies form data handling. Effortlessly parse incoming form submissions and extracting its data.",
    "keywords": [
        "http",
        "form",
        "async",
        "non-blocking",
        "amp",
        "amphp",
        "revolt"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Lowrey",
            "email": "rdlowrey@php.net"
        },
        {
            "name": "Bob Weinand"
        },
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        },
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "amphp/amp": "^3",
        "amphp/byte-stream": "^2",
        "amphp/http": "^2",
        "amphp/http-server": "^3.1",
        "amphp/pipeline": "^1",
        "revolt/event-loop": "^1"
    },
    "require-dev": {
        "amphp/php-cs-fixer-config": "^2",
        "amphp/phpunit-util": "^3",
        "phpunit/phpunit": "^9",
        "amphp/log": "^2",
        "psalm/phar": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Amp\\Http\\Server\\FormParser\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Http\\Server\\FormParser\\Test\\": "test"
        }
    },
	"scripts": {
        "test": "@php -dzend.assertions=1 -dassert.exception=1 ./vendor/bin/phpunit",
        "code-style": "@php ./vendor/bin/php-cs-fixer fix"
    }
}
